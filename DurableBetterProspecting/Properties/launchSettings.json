{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "Client": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "commandLineArgs": "\"$(VINTAGE_STORY)/Vintagestory.dll\" --tracelog --dataPath \"$(ProjectDir)run\" --addModPath \"$(ProjectDir)bin/$(Configuration)\" --addOrigin \"$(ProjectDir)assets\"",
      "workingDirectory": "$(VINTAGE_STORY)"
    },
    "Server": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "commandLineArgs": "\"$(VINTAGE_STORY)/VintagestoryServer.dll\" --tracelog --dataPath \"$(ProjectDir)run\" --addModPath \"$(ProjectDir)bin/$(Configuration)\" --addOrigin \"$(ProjectDir)assets\"",
      "workingDirectory": "$(VINTAGE_STORY)"
    }
  }
}
